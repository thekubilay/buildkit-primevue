{
  "name": "buildkit-primevue",
  "version": "0.1.0",
  "type": "module",
  "description": "FormKit components and helpers built on PrimeVue v4 with Tailwind CSS",
  "keywords": [
    "vue",
    "primevue",
    "forms",
    "form",
    "components",
    "tailwind",
    "vueuse"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://example.com/buildkit-primevue.git"
  },
  "homepage": "https://example.com/buildkit-primevue#readme",
  "bugs": {
    "url": "https://example.com/buildkit-primevue/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --host --port=3000",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "@primeuix/themes": "^1.2.3",
    "@primeuix/utils": ">=0.0.1",
    "@primevue/forms": ">=4.0.0",
    "@primevue/themes": ">=4.0.0",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^24.3.1",
    "@vueuse/core": ">=11.0.0",
    "@tailwindcss/postcss": "^4.1.13",
    "autoprefixer": "^10.4.21",
    "axios": ">=1.0.0",
    "postcss": "^8.5.6",
    "primevue": "^4.3.9",
    "quill": "^2.0.3",
    "tailwindcss": ">=4.0.0",
    "tailwindcss-primeui": "^0.6.1",
    "vue": ">=3.4",
    "zod": ">=3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vue-tsc": "^3.0.5"
  }
}
