{
  "name": "buildkit-primevue",
  "version": "0.1.39",
  "type": "module",
  "description": "FormKit components and helpers built on PrimeVue v4 with Tailwind CSS",
  "keywords": [
    "vue",
    "primevue",
    "forms",
    "form",
    "components",
    "tailwind",
    "vueuse"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thekubilay/buildkit-primevue.git"
  },
  "homepage": "",
  "bugs": {
    "url": "https://github.com/thekubilay/buildkit-primevue/issues"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./FormKit": {
      "types": "./types/FormKit.d.ts",
      "import": "./dist/FormKit.mjs",
      "require": "./dist/FormKit.cjs"
    },
    "./FormKitControl": {
      "types": "./types/FormKitControl.d.ts",
      "import": "./dist/FormKitControl.mjs",
      "require": "./dist/FormKitControl.cjs"
    },
    "./utils/setDynamicFields": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./utils/setFields": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./utils/getPayload": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./utils/clear": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./utils": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./types/FormKitProps": {
      "types": "./types/index.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "vite --host --port=3000",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:ui": "vitest"
  },
  "peerDependencies": {
    "@primeuix/themes": "^1.2.3",
    "@primeuix/utils": ">=0.0.1",
    "@primevue/forms": ">=4.0.0",
    "@primevue/themes": ">=4.0.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^24.3.1",
    "@vueuse/core": "^13.9.0",
    "autoprefixer": "^10.4.21",
    "axios": ">=1.0.0",
    "postcss": "^8.5.6",
    "primevue": "^4.3.9",
    "quill": "^2.0.3",
    "tailwindcss": ">=4.1.13",
    "tailwindcss-primeui": "^0.6.1",
    "vue": ">=3.4",
    "zod": ">=3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.10",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  },
  "dependencies": {
    "buildkit-primevue": "^0.1.38"
  }
}
